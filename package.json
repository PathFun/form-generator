{
  "name": "vite-template",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "fix": "yarn fix:lint && yarn fix:style",
    "fix:lint": "yarn lint:script --fix",
    "fix:style": "stylelint --fix  **/*.{css,less,scss,vue}",
    "lint": "yarn lint:script && yarn lint:style && yarn lint:ls",
    "lint:script": "eslint **/*.{vue,js,ts,tsx}",
    "lint:style": "stylelint  **/*.{css,less,scss,vue}",
    "lint:ls": "ls-lint",
    "prepare": "husky install"
  },
  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/eslint-parser": "^7.18.2",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@ls-lint/ls-lint": "^1.11.2",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/eslint-config-prettier": "^7.0.0",
    "eslint": "^8.19.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.1.1",
    "husky": "^8.0.1",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lint-staged": "^13.0.3",
    "postcss-html": "^1.4.1",
    "postcss-less": "^6.0.0",
    "postcss-scss": "^4.0.4",
    "prettier": "^2.7.1",
    "stylelint": "^14.9.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-order": "^5.0.0",
    "vite": "^2.9.9",
    "vite-plugin-eslint": "^1.6.1"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": "yarn lint:script",
    "*.js": "eslint --cache --fix",
    "*.less": [
      "stylelint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
